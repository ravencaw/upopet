<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="upopet.list">
      <field name="name">upopet list</field>
      <field name="model">upopet.persona</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>

    
    <record model="ir.actions.act_window" id="upopet.action_window">
      <field name="name">upopet window</field>
      <field name="res_model">upopet.persona</field>
      <field name="view_mode">tree,form</field>
    </record>
   

    
    <record model="ir.actions.server" id="upopet.action_server">
      <field name="name">upopet server</field>
      <field name="model_id" ref="model_upopet_upopet"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
   

    
    <menuitem name="upopet" id="upopet.menu_root"/>
   
    
    <menuitem name="Menu 1" id="upopet.menu_1" parent="upopet.menu_root"/>
    <menuitem name="Menu 2" id="upopet.menu_2" parent="upopet.menu_root"/>
   
    
    <menuitem name="List" id="upopet.menu_1_list" parent="upopet.menu_1"
              action="upopet.action_window"/>
    <menuitem name="Server to list" id="upopet" parent="upopet.menu_2"
              action="upopet.action_server"/>
  </data>
</odoo>